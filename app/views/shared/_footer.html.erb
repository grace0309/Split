      <li class="list-inline-item">
        <a class="btn-floating btn-fb mx-1">
        <%= link_to root_path do %>
          <i class="fas fa-home"></i>
        <% end %>
        </a>
      </li>
      <li class="list-inline-item">
        <a class="btn-floating btn-tw mx-1">
